# Google Gemini API Key (required)
GEMINI_API_KEY=your-gemini-api-key-here
# Fallback Gemini API Key (optional - used when primary hits quota limits)
GEMINI_API_KEY_2=

# Agent behavior
DRY_RUN=true
BATCH_SIZE=10
CONFIDENCE_THRESHOLD=0.8

# Whitelist (comma-separated) - emails from these are always important
WHITELIST_DOMAINS=deeplearning.ai,maven.com
WHITELIST_SENDERS=

# Opik Observability (optional - no PII is sent)
OPIK_ENABLED=false
OPIK_API_KEY=
OPIK_PROJECT=email-agent
OPIK_WORKSPACE=

